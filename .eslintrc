{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": [2, "global"],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [2, {"args": "none"}]
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  }
}
